{
    "name": "lamoda/lamoda-b2b-platform.php-sdk",
    "license": "Apache-2.0",
    "type": "library",
    "description": "SDK for interacting with Lamoda B2B Platform",
    "autoload": {
        "psr-4": {
            "LamodaB2B\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\LamodaB2B\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "1.*",
        "guzzlehttp/guzzle": "~6.0",
        "lamoda/lamoda-b2b-platform.dto": "~1.0",
        "doctrine/orm": "~2.2,>=2.2.3"
    },
    "suggest": {
        "kupishoes/leos.partner.php-sdk-bundle": ""
    },
    "repositories": {
        "leos.partner.dto": {
            "type": "vcs",
            "url": "git@github.com:lamoda/lamoda-b2b-platform.dto.git"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    }
}